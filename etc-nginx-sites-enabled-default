server {
        listen 80;
        server_name takeappo.com;
        rewrite ^(.*) http://www.takeappo.com$1 permanent;
}
server {
        listen 80;
        server_name www.takeappo.com;
        root /ms/ta-prog-repos/ta-wa-pt-frontend/script/app/;
        index index.php index.html index.htm;
        client_max_body_size 1000M;
        location / {
                try_files $uri $uri/ /index.php?$query_string;
        }
}